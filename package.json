{
  "name": "@mirite/csv-json-parser",
  "packageManager": "yarn@4.5.0",
  "type": "module",
  "collaborators": [
    "Jesse Conner <me@jesseconner.ca>"
  ],
  "description": "A package for parsing CSV files into JSON",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "wasm-pack build --target web --out-dir ./dist/pkg && tsc",
    "lint": "eslint --fix .",
    "format": "prettier --write . & cargo fmt",
    "test": "cargo test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mirite/csv-json-parser.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@mirite/eslint-config-mirite": "^0.0.5",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "typescript": "^5.6.3"
  }
}
